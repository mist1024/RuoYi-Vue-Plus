<!----------   wxs start ----------->
<wxs module="computed" src="./index.wxs"></wxs>
<!----------   wxs end   ----------->


<view class="{{ computed.rootClass({ classPrefix, name }) }}" style="{{ computed.rootStyle({ customStyle, color, size }) }}" bindtap="onClick">
  <van-info wx:if="{{ info !== null || dot }}" dot="{{ dot }}" info="{{ info }}" custom-class="van-icon__info"></van-info>
  <image wx:if="{{ computed.isImage(name) }}" src="{{ name }}" mode="aspectFit" class="van-icon__image"></image>
</view>
