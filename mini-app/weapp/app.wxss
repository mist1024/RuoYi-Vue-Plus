.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.buttonColor {
  background: #AC1630;
  color: #fff;
}
.buttonColor-cancel {
  background: #fff;
  color: #333;
}
.background-color {
  background: #AC1630;
}
.cell-title {
  padding: 10px 16px;
  font-weight: bold;
}
.cell {
  margin-top: 10px;
}
.boom {
  background-color: #F0F1F2;
  width: 100%;
  height: 15px;
}

